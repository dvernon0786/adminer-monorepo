{
  "version": 2,
  "builds": [
    {
      "src": "adminer/apps/api/api/**/*.js",
      "use": "@vercel/node@3.2.0"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/adminer/apps/api/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}