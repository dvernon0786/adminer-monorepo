{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd adminer/apps/api && npm install && npm run build",
  "outputDirectory": "adminer/apps/api/.next",
  "installCommand": "cd adminer/apps/api && npm install",
  "framework": "nextjs",
  "rewrites": [
    { "source": "/((?!api).*)", "destination": "/" }
  ]
} 