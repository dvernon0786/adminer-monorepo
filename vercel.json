{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "adminer/apps/api/**": {
      "runtime": "nodejs20.x"
    }
  },
  "buildCommand": "bash ../../scripts/vercel-build.sh",
  "framework": "nextjs",
  "env": {
    "CACHE_BUST": "2025-01-24-12:00"
  },
  "build": {
    "env": {
      "CLERK_FRONTEND_API": "",
      "CLERK_PROXY_URL": ""
    }
  },
  "rewrites": [
    { "source": "/dashboard", "destination": "/index.html" },
    { "source": "/dashboard/:path*", "destination": "/index.html" }
  ]
} 