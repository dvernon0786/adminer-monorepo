{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "routes": [
    { "src": "/dashboard", "dest": "/index.html" },
    { "src": "/dashboard/(.*)", "dest": "/index.html" },
    { "src": "/((?!api|_next|favicon.ico).*)", "dest": "/index.html" }
  ]
} 