{
  "version": 2,
  "buildCommand": "npm run build:vercel-fixed",
  "outputDirectory": "public",
  "framework": null,
  "functions": {},
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api|assets|_next|favicon.ico).*)",
      "destination": "/index.html"
    }
  ]
}
