    "client-only": "0.0.1",
    "dayjs": "1.11.13",
    "dotenv": "17.2.1",
    "drizzle-kit": "0.20.9",
    "drizzle-orm": "0.29.3",
    "inngest": "3.40.1",
    "jose": "6.0.13",
    "next": "14.2.10",
    "openai": "4.0.0",
    "pg": "8.16.3",
    "postgres": "3.4.3",
    "raw-body": "3.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "standardwebhooks": "^1.0.0",
    "styled-jsx": "5.1.7",
    "uuid": "9.0.0",
    "zod": "3.22.5"
  },
  "devDependencies": {
