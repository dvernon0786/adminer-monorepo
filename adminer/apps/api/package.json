{
  "name": "@adminer/api",
  "private": true,
  "version": "0.2.2",
  "type": "commonjs",
  "scripts": {
    "dev": "echo 'Static site with API routes - no dev server needed'",
    "build": "npm run build:atomic",
    "build:atomic": "../../scripts/local-atomic-build.sh",
    "build:spa": "cd ../web && echo \"DEBUG: VITE_CLERK_PUBLISHABLE_KEY=$VITE_CLERK_PUBLISHABLE_KEY\" && VITE_CLERK_PUBLISHABLE_KEY=\"$VITE_CLERK_PUBLISHABLE_KEY\" npm ci && VITE_CLERK_PUBLISHABLE_KEY=\"$VITE_CLERK_PUBLISHABLE_KEY\" npm run build && cd ../api && rm -rf public && mkdir -p public && cp -r ../web/dist/* public/",
    "build:vercel": "npm ci && npm run build:atomic",
    "build:vercel-fixed": "npm ci && npm run build:atomic",
    "postbuild": "echo 'Static site build completed'",
    "prestart": "echo 'Static site with API routes ready'",
    "start": "echo 'Static site with API routes - no start server needed'",
    "prevercel": "node ./scripts/guard-spa.mjs",
    "db:check": "node scripts/check-neon-db.cjs",
    "db:migrate": "node scripts/check-neon-db.cjs",
    "db:migrate:dry": "node scripts/check-neon-db.cjs --dry-run",
    "migrate:ts": "node -r ts-node/register -r tsconfig-paths/register scripts/check-neon-db.cjs"
  },
  "dependencies": {
    "@clerk/clerk-js": "5.88.0",
    "@google/generative-ai": "0.24.1",
    "@neondatabase/serverless": "1.0.1",
    "@swc/helpers": "0.5.17",
    "@types/pg": "8.15.5",
    "client-only": "0.0.1",
    "dayjs": "1.11.13",
    "dotenv": "17.2.1",
    "drizzle-kit": "0.20.9",
    "drizzle-orm": "0.29.3",
    "express": "^4.21.2",
    "inngest": "3.40.1",
    "jose": "6.0.13",
    "openai": "4.0.0",
    "pg": "8.16.3",
    "postgres": "3.4.3",
    "raw-body": "3.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "standardwebhooks": "^1.0.0",
    "uuid": "9.0.0",
    "zod": "3.22.5"
  },
  "devDependencies": {
    "@types/node": "20.14.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/uuid": "9.0.8",
    "typescript": "5.5.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
