{
  "name": "@adminer/api",
  "private": true,
  "version": "0.2.2",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:spa && NEXT_EXPORT=false VERCEL_EXPORT=false next build",
    "build:spa": "cd ../web && npm ci && npm run build && cd ../api && rm -rf public && mkdir -p public && cp -r ../web/dist/* public/",
    "postbuild": "echo 'Temporarily disabled: ./scripts/guard-static-export.sh' && echo 'ðŸš¨ CRITICAL: Removing conflicting Next.js HTML files' && rm -f .next/server/pages/404.html .next/server/pages/500.html && echo 'âœ… Removed conflicting HTML files'",
    "prestart": "node ./src/env-check.cjs",
    "start": "next start",
    "prevercel": "node ./scripts/guard-spa.mjs",
    "db:check": "node scripts/check-neon-db.cjs",
    "db:migrate": "node scripts/check-neon-db.cjs",
    "db:migrate:dry": "node scripts/check-neon-db.cjs --dry-run",
    "migrate:ts": "node -r ts-node/register -r tsconfig-paths/register scripts/check-neon-db.cjs"
  },
  "dependencies": {
    "@clerk/clerk-js": "5.88.0",
